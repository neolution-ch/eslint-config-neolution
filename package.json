{
  "name": "@neolution-ch/eslint-config-neolution",
  "version": "2.1.0",
  "description": "This package provides Neolution's .eslintrc as an extensible shared config.",
  "homepage": "https://github.com/neolution-ch/eslint-config-neolution",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "lint": "yarn tsc && eslint --cache",
    "prepack": "yarn tsc",
    "upgradeAll": "npm-check-updates --packageFile ./package.json -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neolution-ch/eslint-config-neolution"
  },
  "license": "MIT",
  "author": "Neolution",
  "dependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/eslintrc": "^3.2.0",
    "@next/eslint-plugin-next": "^15.1.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "typescript-eslint": "^8.20.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@release-it/keep-a-changelog": "^6.0.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/node": "^22.15.32",
    "eslint": "^9.18.0",
    "npm-check-updates": "^16.3.8",
    "prettier": "^3.4.2",
    "release-it": "^18.1.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
