{
  "name": "@neolution-ch/eslint-config-neolution",
  "version": "2.2.0",
  "description": "This package provides Neolution's .eslintrc as an extensible shared config.",
  "homepage": "https://github.com/neolution-ch/eslint-config-neolution",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "lint": "yarn tsc && eslint --cache",
    "prepack": "yarn tsc",
    "upgradeAll": "npm-check-updates --packageFile ./package.json -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neolution-ch/eslint-config-neolution"
  },
  "license": "MIT",
  "author": "Neolution",
  "dependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.5.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsdoc": "^61.1.4",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-unicorn": "^61.0.2",
    "typescript-eslint": "^8.46.1"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@release-it/keep-a-changelog": "^7.0.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^24.7.2",
    "eslint": "^9.37.0",
    "npm-check-updates": "^19.1.1",
    "prettier": "^3.6.2",
    "release-it": "^19.0.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
